{
    "name": "server-yrc",
    "version": "1.0.0",
    "description": "Servidor del Control Remoto de Youtube",
    "main": "index.ts",
    "author": "Enderson Vizcaino",
    "license": "MIT",
    "scripts": {
        "ts-init": "tsc --init",
        "watch": "tsc --watch",
        "help": "ts-node --help",
        "serve": "nodemon -r esm index.ts",
        "watch-all": "concurrently \"npm run watch:client\" \"npm run watch:server\"",
        "watch:client": "webpack -w --config webpack.client.config.js --progress",
        "watch:server": "webpack -w --config webpack.server.config.js --progress",
        "build": "rimraf dist && npm run build:client && npm run build:server",
        "build:client": "webpack --config webpack.client.config.js --progress",
        "build:server": "webpack --config webpack.server.config.js --progress"
    },
    "dependencies": {
        "babel-loader": "^8.0.6",
        "core-util-is": "^1.0.2",
        "express": "^4.17.1",
        "moment": "^2.24.0",
        "rxjs": "^6.5.3",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
        "vue": "^2.6.10",
        "vue-router": "^3.1.3",
        "vue-rx": "^6.2.0",
        "vue-server-renderer": "^2.6.10",
        "vue-socket.io-extended": "^4.0.0",
        "vuex": "^3.1.1",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.0",
        "@types/express": "^4.17.1",
        "@types/socket.io": "^2.1.2",
        "concurrently": "^4.1.2",
        "cross-env": "^5.2.1",
        "css-loader": "^3.2.0",
        "esm": "^3.2.25",
        "file-loader": "^4.2.0",
        "node-sass": "^4.12.0",
        "nodemon": "^1.19.2",
        "rimraf": "^3.0.0",
        "sass-loader": "^8.0.0",
        "ts-loader": "^6.0.4",
        "ts-node": "^8.3.0",
        "typescript": "^3.6.2",
        "vue-loader": "^15.7.1",
        "vue-style-loader": "^4.1.2",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.8",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    }
}
